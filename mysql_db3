CREATE table staff(
    F_Name VARCHAR(20),
    Title VARCHAR(20),
    YOS INT,
    DOB DATE);

INSERT INTO staff(F_Name,Title,YOS,DOB) VALUES
('Manish','Programmer',5,'1975-10-10'),
('Saman','Web Developer',6,'1976-03-05'),
('Camelia','Java Developer',3,'1980-05-06'),
('Kamal','Clerk',10,'1968-10-03'),
('Malan','Software Engineer',5,'1975-06-07');

ALTER TABLE staff RENAME TO STAFFDATA;
ALTER TABLE STAFFDATA ADD COLUMN Salary INT;
DESCRIBE STAFFDATA;

ALTER TABLE STAFFDATA  MODIFY COLUMN Salary FLOAT(10,2);
ALTER TABLE STAFFDATA  CHANGE COLUMN YOS SERVICE CHAR(20);
DESCRIBE STAFFDATA;

ALTER TABLE STAFFDATA ADD COLUMN SerialNumber BOOLEAN;
DESCRIBE STAFFDATA;
ALTER TABLE STAFFDATA  MODIFY COLUMN SerialNumber INT NOT NULL;
ALTER TABLE STAFFDATA  DROP COLUMN SerialNumber;

ALTER TABLE STAFFDATA ADD COLUMN SerialNumber INT NOT NULL AUTO_INCREMENT PRIMARY KEY;


